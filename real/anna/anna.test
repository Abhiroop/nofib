#! /bin/sh
#
export ANNADIR; ANNADIR=`pwd`

$1 < big.cor | sum > /tmp/sum-real-$$

cat > /tmp/sum-expected-$$ << EOTHING
12796    49
EOTHING

if diff -w /tmp/sum-real-$$ /tmp/sum-expected-$$ ; then
    /bin/rm /tmp/sum*$$
    exit 0
else
    echo -n '*** sum I got: ' ;      cat /tmp/sum-real-$$ 
    echo -n '*** sum I expected: ' ; cat /tmp/sum-expected-$$ 
    /bin/rm /tmp/sum*$$
    exit 1
fi
