SRCS_LHS = Main.lhs IntLib.lhs Random.lhs Prime.lhs
OBJS_O   = Main.o   IntLib.o   Random.o   Prime.o

/* mersenne607 takes a *long* time! */
NoFibMultiModuleCompileAndRun(primetest,-i primetest.stdin -o1 primetest.stdout)

NoFibHaskellCompile(primetest,Main,lhs)
NoFibHaskellCompile(primetest,IntLib,lhs)
NoFibHaskellCompile(primetest,Random,lhs)
NoFibHaskellCompile(primetest,Prime,lhs)

NoFibDependTarget(primetest, $(SRCS_LHS))
