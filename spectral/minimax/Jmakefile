SRCS_HS= Main.hs Prog.hs Board.hs Wins.hs Tree.hs Game.hs
OBJS_O=  Main.o  Prog.o  Board.o  Wins.o  Tree.o  Game.o 

NoFibMultiModuleCompileAndRun(minimax,-o1 minimax.stdout)

NoFibHaskellCompile(minimax,Main,hs)
NoFibHaskellCompile(minimax,Prog,hs)
NoFibHaskellCompile(minimax,Board,hs)
NoFibHaskellCompile(minimax,Wins,hs)
NoFibHaskellCompile(minimax,Tree,hs)
NoFibHaskellCompile(minimax,Game,hs)

NoFibDependTarget(minimax, $(SRCS_HS))
