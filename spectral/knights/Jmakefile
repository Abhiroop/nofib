SRCS_LHS = Main.lhs KnightHeuristic.lhs ChessSetList.lhs Queue.lhs Sort.lhs
OBJS_O = Main.o KnightHeuristic.o ChessSetList.o Queue.o Sort.o

NoFibMultiModuleCompileAndRun(knights, 8 3 -o1 knights.stdout)

NoFibHaskellCompile(knights,Main,lhs)
NoFibHaskellCompile(knights,KnightHeuristic,lhs)
NoFibHaskellCompile(knights,ChessSetList,lhs)
NoFibHaskellCompile(knights,Queue,lhs)
NoFibHaskellCompile(knights,Sort,lhs)

NoFibDependTarget(knights, $(SRCS_LHS))
