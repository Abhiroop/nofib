SRCS_HS = Main.hs RA.hs RC.hs RG.hs RU.hs Types.hs
OBJS_O  = Main.o  RA.o  RC.o  RG.o  RU.o  Types.o

NoFibMultiModuleCompileAndRun(nucleic2, -o1 nucleic2.stdout1 -o1 nucleic2.stdout2)

NoFibHaskellCompile(nucleic2,Main,hs)
NoFibHaskellCompile(nucleic2,RA,hs)
NoFibHaskellCompile(nucleic2,RC,hs)
NoFibHaskellCompile(nucleic2,RG,hs)
NoFibHaskellCompile(nucleic2,RU,hs)
NoFibHaskellCompile(nucleic2,Types,hs)

NoFibDependTarget(nucleic2, $(SRCS_HS))
